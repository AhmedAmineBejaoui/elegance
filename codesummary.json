      "content": "\r\nimport { useState, useEffect } from \"react\";\r\nimport { User, Mail, Phone, MapPin, Calendar, Package, Heart, CreditCard, Shield, Edit, Save, X } from \"lucide-react\";\r\nimport { FcGoogle } from \"react-icons/fc\";\r\nimport { Card, CardContent, CardHeader, CardTitle } from \"@/components/ui/card\";\r\nimport { Button } from \"@/components/ui/button\";\r\nimport { Input } from \"@/components/ui/input\";\r\nimport { Label } from \"@/components/ui/label\";\r\nimport { Tabs, TabsContent, TabsList, TabsTrigger } from \"@/components/ui/tabs\";\r\nimport { Avatar, AvatarFallback, AvatarImage } from \"@/components/ui/avatar\";\r\nimport { Badge } from \"@/components/ui/badge\";\r\nimport { Separator } from \"@/components/ui/separator\";\r\nimport { useQuery, useMutation, useQueryClient } from \"@tanstack/react-query\";\r\nimport { useAuth } from \"@/hooks/useAuth\";\r\nimport { useToast } from \"@/hooks/use-toast\";\r\nimport { apiRequest } from \"@/lib/queryClient\";\r\nimport { Link } from \"wouter\";\r\n\r\nexport default function ProfilePage() {\r\n  const { user, isAuthenticated, isLoading } = useAuth();\r\n  const { toast } = useToast();\r\n  const queryClient = useQueryClient();\r\n  \r\n  const [isEditing, setIsEditing] = useState(false);\r\n  const [formData, setFormData] = useState({\r\n    firstName: \"\",\r\n    lastName: \"\",\r\n    email: \"\",\r\n    phone: \"\",\r\n    address: \"\",\r\n    city: \"\",\r\n    postalCode: \"\",\r\n  });\r\n\r\n  useEffect(() => {\r\n    if (user) {\r\n      setFormData({\r\n        firstName: user.firstName || \"\",\r\n        lastName: user.lastName || \"\",\r\n        email: user.email || \"\",\r\n        phone: user.phone || \"\",\r\n        address: user.address || \"\",\r\n        city: user.city || \"\",\r\n        postalCode: user.postalCode || \"\",\r\n      });\r\n    }\r\n  }, [user]);\r\n\r\n  const { data: orders = [], isLoading: ordersLoading } = useQuery<any[]>({\r\n    queryKey: [\"/api/orders/user\"],\r\n    enabled: isAuthenticated,\r\n  });\r\n\r\n  const updateProfileMutation = useMutation({\r\n    mutationFn: (data: any) => apiRequest(\"PATCH\", \"/api/auth/profile\", data),\r\n    onSuccess: () => {\r\n      queryClient.invalidateQueries({ queryKey: [\"/api/auth/user\"] });\r\n      setIsEditing(false);\r\n      toast({\r\n        title: \"Profil mis à jour\",\r\n        description: \"Vos informations ont été sauvegardées avec succès.\",\r\n      });\r\n    },\r\n    onError: () => {\r\n      toast({\r\n        title: \"Erreur\",\r\n        description: \"Impossible de mettre à jour le profil.\",\r\n        variant: \"destructive\",\r\n      });\r\n    },\r\n  });\r\n\r\n  const handleSave = () => {\r\n    updateProfileMutation.mutate(formData);\r\n  };\r\n\r\n  const handleCancel = () => {\r\n    if (user) {\r\n      setFormData({\r\n        firstName: user.firstName || \"\",\r\n        lastName: user.lastName || \"\",\r\n        email: user.email || \"\",\r\n        phone: user.phone || \"\",\r\n        address: user.address || \"\",\r\n        city: user.city || \"\",\r\n        postalCode: user.postalCode || \"\",\r\n      });\r\n    }\r\n    setIsEditing(false);\r\n  };\r\n\r\n  if (isLoading) {\r\n    return (\r\n      <div className=\"min-h-screen flex items-center justify-center\">\r\n        <div className=\"animate-spin rounded-full h-12 w-12 border-b-2 border-primary\"></div>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  if (!isAuthenticated) {\r\n    return (\r\n      <div className=\"min-h-screen flex items-center justify-center bg-gray-50\">\r\n        <Card className=\"w-full max-w-md\">\r\n          <CardContent className=\"p-8 text-center\">\r\n            <User className=\"mx-auto h-12 w-12 text-gray-400 mb-4\" />\r\n            <h2 className=\"text-xl font-semibold mb-2\">Connexion requise</h2>\r\n            <p className=\"text-gray-600 mb-4\">\r\n              Vous devez être connecté pour accéder à votre profil.\r\n            </p>\r\n            <Link href=\"/auth\">\r\n              <Button className=\"w-full\">Se connecter</Button>\r\n            </Link>\r\n          </CardContent>\r\n        </Card>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  return (\r\n    <div className=\"min-h-screen bg-gray-50 py-8\">\r\n      <div className=\"max-w-6xl mx-auto px-4 sm:px-6 lg:px-8\">\r\n        {/* Header */}\r\n        <div className=\"mb-8\">\r\n          <div className=\"flex items-center justify-between\">\r\n            <div>\r\n              <h1 className=\"text-3xl font-bold text-gray-900\">Mon Profil</h1>\r\n              <p className=\"text-gray-600\">Gérez vos informations personnelles et commandes</p>\r\n            </div>\r\n            <Button onClick={() => window.location.href = \"/api/logout\"} variant=\"outline\">\r\n              Déconnexion\r\n            </Button>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"grid grid-cols-1 lg:grid-cols-4 gap-8\">\r\n          {/* Sidebar */}\r\n          <div className=\"lg:col-span-1\">\r\n            <Card>\r\n              <CardContent className=\"p-6\">\r\n                <div className=\"flex flex-col items-center text-center\">\r\n                  <Avatar className=\"w-20 h-20 mb-4\">\r\n                    <AvatarImage src={user?.profileImageUrl} />\r\n                    <AvatarFallback className=\"bg-gradient-to-r from-pink-500 to-purple-600 text-white text-lg\">\r\n                      {user?.firstName?.[0]}{user?.lastName?.[0]}\r\n                    </AvatarFallback>\r\n                  </Avatar>\r\n                  <h3 className=\"font-semibold text-lg\">{user?.firstName} {user?.lastName}</h3>\r\n                  <p className=\"text-gray-600 text-sm\">{user?.email}</p>\r\n                  <Badge variant=\"secondary\" className=\"mt-2\">\r\n                    {user?.role === 'admin' ? 'Administrateur' : 'Client'}\r\n                  </Badge>\r\n                </div>\r\n\r\n                <Separator className=\"my-6\" />\r\n\r\n                <div className=\"space-y-4\">\r\n                  <div className=\"flex items-center space-x-3 text-sm\">\r\n                    <Calendar className=\"w-4 h-4 text-gray-400\" />\r\n                    <span>Membre depuis {new Date(user?.createdAt || '').toLocaleDateString()}</span>\r\n                  </div>\r\n                  <div className=\"flex items-center space-x-3 text-sm\">\r\n                    <Package className=\"w-4 h-4 text-gray-400\" />\r\n                    <span>{orders.length} commande{orders.length > 1 ? 's' : ''}</span>\r\n                  </div>\r\n                </div>\r\n              </CardContent>\r\n            </Card>\r\n          </div>\r\n\r\n          {/* Main Content */}\r\n          <div className=\"lg:col-span-3\">\r\n            <Tabs defaultValue=\"profile\" className=\"space-y-6\">\r\n              <TabsList className=\"grid w-full grid-cols-3\">\r\n                <TabsTrigger value=\"profile\" className=\"flex items-center space-x-2\">\r\n                  <User className=\"w-4 h-4\" />\r\n                  <span>Profil</span>\r\n                </TabsTrigger>\r\n                <TabsTrigger value=\"orders\" className=\"flex items-center space-x-2\">\r\n                  <Package className=\"w-4 h-4\" />\r\n                  <span>Commandes</span>\r\n                </TabsTrigger>\r\n                <TabsTrigger value=\"security\" className=\"flex items-center space-x-2\">\r\n                  <Shield className=\"w-4 h-4\" />\r\n                  <span>Sécurité</span>\r\n                </TabsTrigger>\r\n              </TabsList>\r\n\r\n              {/* Profile Tab */}\r\n              <TabsContent value=\"profile\">\r\n                <Card>\r\n                  <CardHeader className=\"flex flex-row items-center justify-between\">\r\n                    <CardTitle>Informations personnelles</CardTitle>\r\n                    {!isEditing ? (\r\n                      <Button onClick={() => setIsEditing(true)} variant=\"outline\" size=\"sm\">\r\n                        <Edit className=\"w-4 h-4 mr-2\" />\r\n                        Modifier\r\n                      </Button>\r\n                    ) : (\r\n                      <div className=\"flex space-x-2\">\r\n                        <Button onClick={handleSave} size=\"sm\" disabled={updateProfileMutation.isPending}>\r\n                          <Save className=\"w-4 h-4 mr-2\" />\r\n                          Sauvegarder\r\n                        </Button>\r\n                        <Button onClick={handleCancel} variant=\"outline\" size=\"sm\">\r\n                          <X className=\"w-4 h-4 mr-2\" />\r\n                          Annuler\r\n                        </Button>\r\n                      </div>\r\n                    )}\r\n                  </CardHeader>\r\n                  <CardContent className=\"space-y-6\">\r\n                    <div className=\"grid grid-cols-1 md:grid-cols-2 gap-6\">\r\n                      <div className=\"space-y-2\">\r\n                        <Label htmlFor=\"firstName\">Prénom</Label>\r\n                        <Input\r\n                          id=\"firstName\"\r\n                          value={formData.firstName}\r\n                          onChange={(e) => setFormData(prev => ({ ...prev, firstName: e.target.value }))}\r\n                          disabled={!isEditing}\r\n                        />\r\n                      </div>\r\n                      <div className=\"space-y-2\">\r\n                        <Label htmlFor=\"lastName\">Nom</Label>\r\n                        <Input\r\n                          id=\"lastName\"\r\n                          value={formData.lastName}\r\n                          onChange={(e) => setFormData(prev => ({ ...prev, lastName: e.target.value }))}\r\n                          disabled={!isEditing}\r\n                        />\r\n                      </div>\r\n                      <div className=\"space-y-2\">\r\n                        <Label htmlFor=\"email\">Email</Label>\r\n                        <Input\r\n                          id=\"email\"\r\n                          type=\"email\"\r\n                          value={formData.email}\r\n                          disabled={true}\r\n                          className=\"bg-gray-50\"\r\n                        />\r\n                      </div>\r\n                      <div className=\"space-y-2\">\r\n                        <Label htmlFor=\"phone\">Téléphone</Label>\r\n                        <Input\r\n                          id=\"phone\"\r\n                          value={formData.phone}\r\n                          onChange={(e) => setFormData(prev => ({ ...prev, phone: e.target.value }))}\r\n                          disabled={!isEditing}\r\n                        />\r\n                      </div>\r\n                      <div className=\"space-y-2 md:col-span-2\">\r\n                        <Label htmlFor=\"address\">Adresse</Label>\r\n                        <Input\r\n                          id=\"address\"\r\n                          value={formData.address}\r\n                          onChange={(e) => setFormData(prev => ({ ...prev, address: e.target.value }))}\r\n                          disabled={!isEditing}\r\n                        />\r\n                      </div>\r\n                      <div className=\"space-y-2\">\r\n                        <Label htmlFor=\"city\">Ville</Label>\r\n                        <Input\r\n                          id=\"city\"\r\n                          value={formData.city}\r\n                          onChange={(e) => setFormData(prev => ({ ...prev, city: e.target.value }))}\r\n                          disabled={!isEditing}\r\n                        />\r\n                      </div>\r\n                      <div className=\"space-y-2\">\r\n                        <Label htmlFor=\"postalCode\">Code postal</Label>\r\n                        <Input\r\n                          id=\"postalCode\"\r\n                          value={formData.postalCode}\r\n                          onChange={(e) => setFormData(prev => ({ ...prev, postalCode: e.target.value }))}\r\n                          disabled={!isEditing}\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                  </CardContent>\r\n                </Card>\r\n              </TabsContent>\r\n\r\n              {/* Orders Tab */}\r\n              <TabsContent value=\"orders\">\r\n                <Card>\r\n                  <CardHeader>\r\n                    <CardTitle>Historique des commandes</CardTitle>\r\n                  </CardHeader>\r\n                  <CardContent>\r\n                    {ordersLoading ? (\r\n                      <div className=\"flex justify-center py-8\">\r\n                        <div className=\"animate-spin rounded-full h-8 w-8 border-b-2 border-primary\"></div>\r\n                      </div>\r\n                    ) : orders.length === 0 ? (\r\n                      <div className=\"text-center py-8\">\r\n                        <Package className=\"mx-auto h-12 w-12 text-gray-400 mb-4\" />\r\n                        <h3 className=\"text-lg font-medium text-gray-900 mb-2\">Aucune commande</h3>\r\n                        <p className=\"text-gray-600 mb-4\">Vous n'avez pas encore passé de commande.</p>\r\n                        <Link href=\"/products\">\r\n                          <Button>Découvrir nos produits</Button>\r\n                        </Link>\r\n                      </div>\r\n                    ) : (\r\n                      <div className=\"space-y-4\">\r\n                        {orders.map((order: any) => (\r\n                          <div key={order.id} className=\"border rounded-lg p-4 hover:bg-gray-50\">\r\n                            <div className=\"flex items-center justify-between mb-3\">\r\n                              <div>\r\n                                <h4 className=\"font-medium\">Commande #{order.orderNumber}</h4>\r\n                                <p className=\"text-sm text-gray-600\">\r\n                                  {new Date(order.createdAt).toLocaleDateString('fr-FR', {\r\n                                    year: 'numeric',\r\n                                    month: 'long',\r\n                                    day: 'numeric'\r\n                                  })}\r\n                                </p>\r\n                              </div>\r\n                              <div className=\"text-right\">\r\n                                <p className=\"font-semibold\">{parseFloat(order.total).toFixed(2)} DT</p>\r\n                                <Badge variant={\r\n                                  order.status === 'delivered' ? 'default' :\r\n                                  order.status === 'pending' ? 'secondary' : 'outline'\r\n                                }>\r\n                                  {order.status === 'pending' ? 'En attente' :\r\n                                   order.status === 'processing' ? 'En traitement' :\r\n                                   order.status === 'shipped' ? 'Expédiée' :\r\n                                   order.status === 'delivered' ? 'Livrée' : 'Annulée'}\r\n                                </Badge>\r\n                              </div>\r\n                            </div>\r\n                            <div className=\"flex items-center justify-between\">\r\n                              <span className=\"text-sm text-gray-600\">\r\n                                {order.items?.length || 0} article{(order.items?.length || 0) > 1 ? 's' : ''}\r\n                              </span>\r\n                              <Link href={`/orders`}>\r\n                                <Button variant=\"outline\" size=\"sm\">\r\n                                  Voir détails\r\n                                </Button>\r\n                              </Link>\r\n                            </div>\r\n                          </div>\r\n                        ))}\r\n                      </div>\r\n                    )}\r\n                  </CardContent>\r\n                </Card>\r\n              </TabsContent>\r\n\r\n              {/* Security Tab */}\r\n              <TabsContent value=\"security\">\r\n                <Card>\r\n                  <CardHeader>\r\n                    <CardTitle>Sécurité et confidentialité</CardTitle>\r\n                  </CardHeader>\r\n                  <CardContent className=\"space-y-6\">\r\n                    <div className=\"bg-emerald-50 p-4 rounded-lg\">\r\n                      <div className=\"flex items-center space-x-3\">\r\n                        <Shield className=\"w-5 h-5 text-emerald-600\" />\r\n                        <div>\r\n                          <h4 className=\"font-medium text-emerald-900\">Compte sécurisé</h4>\r\n                          <p className=\"text-sm text-emerald-700\">\r\n                            Votre compte est protégé par l'authentification Google\r\n                          </p>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div className=\"space-y-4\">\r\n                      <div>\r\n                        <h4 className=\"font-medium mb-2\">Méthode de connexion</h4>\r\n                        <div className=\"flex items-center space-x-3 p-3 border rounded-lg\">\r\n                          <FcGoogle className=\"w-8 h-8\" />\r\n                          <div>\r\n                            <p className=\"font-medium\">Google Auth</p>\r\n                            <p className=\"text-sm text-gray-600\">Connexion sécurisée via Google</p>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n\r\n                      <div>\r\n                        <h4 className=\"font-medium mb-2\">Données de compte</h4>\r\n                        <p className=\"text-sm text-gray-600 mb-3\">\r\n                          Gérez vos données personnelles et vos préférences de confidentialité.\r\n                        </p>\r\n                        <Button variant=\"outline\" onClick={() => window.location.href = \"/api/logout\"}>\r\n                          Se déconnecter\r\n                        </Button>\r\n                      </div>\r\n                    </div>\r\n                  </CardContent>\r\n                </Card>\r\n              </TabsContent>\r\n            </Tabs>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n",
