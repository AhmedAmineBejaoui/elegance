{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/[...all].js": {
      "includeFiles": "dist/server/**"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[...all].js" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
